{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "CI",
    "NODE_ENV",
    "ALCHEMY_ID",
    "GITHUB_TOKEN",
    "NEXT_PUBLIC_ALCHEMY_ID",
    "SKIP_BUILD_STATIC_GENERATION"
  ],
  "globalDependencies": [
    ".eslintignore",
    ".eslint.js",
    ".npmrc",
    ".nvmrc",
    ".prettierignore",
    ".prettierrc.js",
    "tsconfig.json"
  ],
  "pipeline": {
    "db:init": {
      "outputs": ["prisma/**/*"],
      "cache": false
    },
    "db:pull": {
      "outputs": ["prisma/**/*"],
      "dependsOn": ["db:init"],
      "cache": false,
      "env": ["DATABASE_URL"]
    },
    "db:generate": {
      "dependsOn": [],
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "playwright": {
      "cache": false,
      "env": ["ANVIL_BLOCK_NUMBER", "ANVIL_FORK_URL", "PLAYWRIGHT_URL", "NEXT_PUBLIC_PLAYWRIGHT_ENABLED", "CHAIN_ID"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**/*"],
      "env": ["ANVIL_BLOCK_NUMBER", "ANVIL_FORK_URL", "PLAYWRIGHT_URL", "NEXT_PUBLIC_PLAYWRIGHT_ENABLED", "CHAIN_ID"]
    },
    "test-e2e-playwright": {
      "cache": false,
      "env": ["ANVIL_BLOCK_NUMBER", "ANVIL_FORK_URL", "PLAYWRIGHT_URL", "NEXT_PUBLIC_PLAYWRIGHT_ENABLED", "CHAIN_ID"]
    },
    "blog#build": {
      "dependsOn": ["^build", "generate"],
      "env": [
        "STRAPI_PREVIEW_SECRET"
      ],
      "outputs": [
        ".mesh/**/*", 
        ".next/**/*", 
        "public/**/*"
      ]
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "start": {
      "dependsOn": ["^build", "^db:generate", "generate"],
      "cache": false
    },
    "generate": {
      "outputs": [".graphclient/**/*", ".mesh/**/*", "typechain/**/*", "artifacts/**/*", "cache/**/*"]
    },
    "build": {
      "outputs": [
        ".vercel/**/*",
        ".next/**/*",
        ".graphclient/**/*",
        ".mesh/**/*",
        "dist/**/*",
        "typechain/**/*",
        "artifacts/**/*",
        "cache/**/*",
        "public/**/*",
        "storybook-static/**/*"
      ],
      "dependsOn": ["^db:generate", "^build", "db:generate", "generate"],
      "env": [
        "ROOT_URL",
        "ACADEMY_URL",
        "ANALYTICS_URL",
        "BLOG_URL",
        "BRIDGE_URL",
        "EARN_URL",
        "FURO_URL",
        "INTERNAL_URL",
        "KASHI_URL",
        "PARTNER_URL",
        "SWAP_URL",
        "XSWAP_URL",
        "DATABASE_URL",
        "REDIS_URL",
        "NEXT_PUBLIC_EARN_URL"
      ]
    },
    "clean": {
      "cache": false
    },
    "check": {
      "dependsOn": [
        "//#generate",
        "//#build-config", 
        "//#build-packages", 
        "//#build-protocols"
      ],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^build", "^db:generate", "generate"],
      "cache": false
    },
    "validate": {
      "cache": false
    },
    "anvil": {
      "cache": false,
      "env": ["ANVIL_BLOCK_NUMBER", "ANVIL_FORK_URL"]
    },
    "export": {
      "dependsOn": ["build"],
      "outputs": ["exports.json", "exports.ts"],
      "cache": true
    },
    "node": {
      "dependsOn": ["^build"],
      "outputs": ["deployments/**/*"]
    },
    "server": {
      "dependsOn": ["^build"]
    },
    "//#generate": {
      "dependsOn": [],
      "outputs": [
        "typechain/**/*", 
        ".graphclient/**/*", 
        ".mesh/**/*"
      ],
      "inputs": []
    },
    "//#build-config": {
      "dependsOn": [],
      "outputs": ["dist/**/*"],
      "inputs": []
    },
    "//#build-packages": {
      "dependsOn": [],
      "outputs": [
        ".graphclient/**/*",
        "dist/**/*"
      ],
      "inputs": []
    },
    "//#build-protocols": {
      "dependsOn": [],
      "outputs": ["typechain/**/*", "artifacts/**/*", "cache/**/*"],
      "inputs": [
        "contracts/**/*"
      ]
    }
  }
}
